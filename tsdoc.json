{
	"tags":
	{
		"allowUnknownTags" : true
	},
	"plugins":[
		 "plugins/markdown"
		,"/usr/local/lib/node_modules/tsdoc/template/plugins/TSDoc.js"
	],
	"opts":
	{
		"template"	:"/usr/local/lib/node_modules/tsdoc/template",
		"recurse"	:"true"
	},
	"templates" : {
		"cleverLinks"     : false,
		"monospaceLinks"  : false
	},
	"source":
	{
		"exclude": [
			".git",
			".meteor",
			"node_modules"
		],
		"includePattern": "(\\.d)?\\.tsx?$"
	},
	"markdown"  : {
		"parser"   : "gfm",
		"hardwrap" : true
	},
	"tsdoc":{
		"source"		:"/Users/carletonmoore/RadGrad/radgrad2/app/",
		"destination"	:"/Users/carletonmoore/RadGrad/radgrad2/docs",
		"tutorials"		:"",
		"systemName"	: "app",
		"footer"		: "",
		"copyright"		: "app Copyright Â© 2019 carletonmoore.",
		"outputSourceFiles" : true,
		"commentsOnly": true
	}
}
